// https://leetcode.com/problems/delete-columns-to-make-sorted/

class Solution {
    public int minDeletionSize(String[] A) {
        int c = 0;
        for (int i=0; i<A[0].length(); i++) {
            int f = 0;
            for (int j=0; j<A.length-1; j++) {
                if ((int)A[j].charAt(i) > (int)A[j+1].charAt(i)) {
                    c ++;
                    break;
                }
            }
        }
        return c;
    }
}
